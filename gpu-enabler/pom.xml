        <plugin>
          <groupId>org.scalatest</groupId>
          <artifactId>scalatest-maven-plugin</artifactId>
          <version>1.0</version>
          <configuration>
            <reportsDirectory>${project.build.directory}/surefire-reports</reportsDirectory>
            <junitxml>.</junitxml>
            <forkMode>once</forkMode>
            <filereports>WDF TestSuite.txt</filereports>
            <argLine>-Djava.library.path=${project.build.directory}/lib/ -Xms12g -Xmx25G -Xmn10G</argLine>
          </configuration>
          <executions>
            <execution>
              <id>test</id>
              <goals>
                <goal>test</goal>
              </goals>
            </execution>
          </executions>
        </plugin>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-shade-plugin</artifactId>
        <version>2.4.3</version>
        <configuration>
          <shadedArtifactAttached>false</shadedArtifactAttached>
          <transformers>
              <transformer implementation="org.apache.maven.plugins.shade.resource.IncludeResourceTransformer">
                  <resource>lib/libJCudaDriver-${jcuda.lib.suffix}.${jcuda.lib.ext}</resource>
                  <file>${project.build.directory}/lib/libJCudaDriver-${jcuda.lib.suffix}.${jcuda.lib.ext}</file>
              </transformer>
              <transformer implementation="org.apache.maven.plugins.shade.resource.IncludeResourceTransformer">
                  <resource>lib/libJCudaRuntime-${jcuda.lib.suffix}.${jcuda.lib.ext}</resource>
                  <file>${project.build.directory}/lib/libJCudaRuntime-${jcuda.lib.suffix}.${jcuda.lib.ext}</file>
              </transformer>
              <transformer implementation="org.apache.maven.plugins.shade.resource.IncludeResourceTransformer">
                  <resource>lib/libJCublas-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</resource>
                  <file>${project.build.directory}/lib/libJCublas-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</file>
              </transformer>
              <transformer implementation="org.apache.maven.plugins.shade.resource.IncludeResourceTransformer">
                  <resource>lib/libJCublas2-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</resource>
                  <file>${project.build.directory}/lib/libJCublas2-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</file>
              </transformer>
              <transformer implementation="org.apache.maven.plugins.shade.resource.IncludeResourceTransformer">
                  <resource>lib/libJCudaRuntime-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</resource>
                  <file>${project.build.directory}/lib/libJCudaRuntime-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</file>
              </transformer>
              <transformer implementation="org.apache.maven.plugins.shade.resource.IncludeResourceTransformer">
                  <resource>lib/libJCudaDriver-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</resource>
                  <file>${project.build.directory}/lib/libJCudaDriver-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</file>
              </transformer>
              <transformer implementation="org.apache.maven.plugins.shade.resource.IncludeResourceTransformer">
                  <resource>lib/libJCudnn-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</resource>
                  <file>${project.build.directory}/lib/libJCudnn-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</file>
              </transformer>
              <transformer implementation="org.apache.maven.plugins.shade.resource.IncludeResourceTransformer">
                  <resource>lib/libJCufft-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</resource>
                  <file>${project.build.directory}/lib/libJCufft-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</file>
              </transformer>
              <transformer implementation="org.apache.maven.plugins.shade.resource.IncludeResourceTransformer">
                  <resource>lib/libJCurand-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</resource>
                  <file>${project.build.directory}/lib/libJCurand-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</file>
              </transformer>
              <transformer implementation="org.apache.maven.plugins.shade.resource.IncludeResourceTransformer">
                  <resource>lib/libJCusolver-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</resource>
                  <file>${project.build.directory}/lib/libJCusolver-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</file>
              </transformer>
              <transformer implementation="org.apache.maven.plugins.shade.resource.IncludeResourceTransformer">
                  <resource>lib/libJCusparse-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</resource>
                  <file>${project.build.directory}/lib/libJCusparse-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</file>
              </transformer>
              <transformer implementation="org.apache.maven.plugins.shade.resource.IncludeResourceTransformer">
                  <resource>lib/libJNvgraph-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</resource>
                  <file>${project.build.directory}/lib/libJNvgraph-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</file>
              </transformer>
              <transformer implementation="org.apache.maven.plugins.shade.resource.IncludeResourceTransformer">
                  <resource>lib/libJNvrtc-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</resource>
                  <file>${project.build.directory}/lib/libJNvrtc-${jcuda.version}-${libJ.version}.${jcuda.lib.ext}</file>
              </transformer>
          </transformers>
          <artifactSet>
             <includes>
               <!-- include>org.jcuda:jcuda</include -->
               <include>org.mystic:mavenized.jcuda</include>
               <include>jcuda:**</include>
             </includes>
            <excludes>
              <exclude>jcuda:*:so:*</exclude>
              <exclude>jcuda:*:dll:*</exclude>
              <exclude>jcuda:*:dylib:*</exclude>
